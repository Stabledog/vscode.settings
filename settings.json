        {
            "before": [
                "<leader>",
                "1"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "<leader>",
                "v"
            ],
            "commands": [
                "open-in-vim.open"
            ]
        },
        {
            "before": [
                "<leader>",
                "n"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "="
            ],
            "commands": [
                "workbench.action.tasks.build"
            ]
        },
        {
            "after": [
                "g",
                "j"
            ],
            "before": [
                "j"
            ]
        },
        {
            "after": [
                "g",
                "k"
            ],
            "before": [
                "k"
            ]
        }
